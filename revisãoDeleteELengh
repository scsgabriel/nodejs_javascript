let vect = ['a', 'b', 'c', 'd', 'e'];
console.log(vect);
delete vect[0];
console.log(vect); //ele deleta o dado, mas mantém o espaço na memória vazio
                   //pra colocarmos outro valor dentro desse espaço de memória
                   //ous seja, ele exclui o dado e mantém o índice
console.log("A quantidade de elementos no vetor é: " + vect.length);
